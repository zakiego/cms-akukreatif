{
  "kind": "collectionType",
  "collectionName": "businesses",
  "info": {
    "singularName": "business",
    "pluralName": "businesses",
    "displayName": "business",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "regency": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::regency.regency"
    },
    "sector": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sector.sector"
    },
    "description": {
      "type": "richtext"
    },
    "social": {
      "type": "component",
      "repeatable": false,
      "component": "social.socials"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::owner.owner",
      "inversedBy": "businesses"
    },
    "address": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "uuid": {
      "type": "customField",
      "regex": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "customField": "plugin::field-uuid.uuid",
      "required": true
    }
  }
}
